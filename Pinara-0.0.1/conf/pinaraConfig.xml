		</myra:stateInfos>
		<userName>testUser@likyateknoloji.com</userName>
		<lik:userPassword>testUserPassword</lik:userPassword>	
		<mailProps>
			<prop>mail.smtp.host,mail.likya.com</prop>
		    <prop>mail.smtp.port,587</prop>
		    <prop>mail.smtp.auth,true</prop>
		    <prop>mail.debug,false</prop>
		</mailProps>
	</mailInfo>
	<mcInfo>
		<jmxPort>4002</jmxPort>
	</mcInfo>
	<logPath>logs</logPath>
	<!--
		if empty value cacheExpire default is 2592000 (30 days in seconds)
		if not defined cache is disabled. To see the effect browser must be restarted  
		<cacheExpire></cacheExpire>
	-->
	<!--
		if empty value maxHistoryLen default is 20
	<maxHistoryLen>20</maxHistoryLen>
	-->
</pinaraConfig>
