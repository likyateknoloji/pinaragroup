	<httpsPort>4443</httpsPort>
	<persistent>false</persistent>
	<mailInfo enabled="false">
	    <from>test@likyateknoloji.com</from>
		<lik:EmailList>
			<lik:email>test2@likyateknoloji.com</lik:email>
		</lik:EmailList>
		<myra:stateInfos>
			<myra:LiveStateInfos>
