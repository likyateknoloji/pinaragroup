					<myra:StateName>FINISHED</myra:StateName>
					<myra:SubstateName>COMPLETED</myra:SubstateName>
					<myra:StatusName>FAILED</myra:StatusName>
				</myra:LiveStateInfo>
				<myra:LiveStateInfo LSIDateTime="2013-12-22T18:44:31.130+02:00">
					<myra:StateName>RUNNING</myra:StateName>
                     <myra:SubstateName>ON-RESOURCE</myra:SubstateName>
